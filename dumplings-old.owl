    <!-- Equivalent Classes (Matching Pizza Ontology Structure) -->
    <EquivalentClasses>
        <Class IRI="dumplings:BoiledDumpling"/>
        <ObjectIntersectionOf>
            <Class IRI="dumplings:Dumpling"/>
            <ObjectAllValuesFrom>
                <ObjectProperty IRI="dumplings:cookedBy"/>
                <Class IRI="dumplings:Boiling"/>
            </ObjectAllValuesFrom>
        </ObjectIntersectionOf>
    </EquivalentClasses>
    <EquivalentClasses>
        <Class IRI="dumplings:DumplingCombo"/>
        <ObjectIntersectionOf>
            <Class IRI="dumplings:Dumpling"/>
            <ObjectMinCardinality cardinality="2">
                <ObjectProperty IRI="dumplings:hasFilling"/>
                <Class IRI="dumplings:Filling"/>
            </ObjectMinCardinality>
            <ObjectExactCardinality cardinality="2">
                <ObjectProperty IRI="dumplings:servedWith"/>
                <Class IRI="dumplings:Sauce"/>
            </ObjectExactCardinality>
        </ObjectIntersectionOf>
    </EquivalentClasses>
    
